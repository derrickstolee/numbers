(set-option :produce-proofs true)
(declare-const size Int)
(declare-const x000 Int)
(assert (< x000 2))
(assert (>= x000 0))
(declare-const x001 Int)
(assert (< x001 2))
(assert (>= x001 0))
(declare-const x002 Int)
(assert (< x002 2))
(assert (>= x002 0))
(declare-const x003 Int)
(assert (< x003 2))
(assert (>= x003 0))
(assert (= size (+ x000 x001 x002 x003)))
(assert (> 3 (+ x000 x001 x002)))
(assert (> 3 (+ x001 x002 x003)))
(assert (>= size 4))
(check-sat)
(get-proof)
